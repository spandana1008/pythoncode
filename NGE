#NGE:Next Greater Element 
#code
arr=list(map(int,input().split()))
n=len(arr)
stack=[]
ans=[]
for ind in range(n-1,-1,-1):
  currEle=arr[ind]
  while(len(stack)!=0 and stack[-1]<=currEle):
    stack.pop()
  if(len(stack)==0):
    ans.append(-1)
  else:
    ans.append(stack[-1])
  stack.append(currEle)
print(ans[::-1])


#output:6 0 8 1 3
#[8, 8, -1, 3, -1]
